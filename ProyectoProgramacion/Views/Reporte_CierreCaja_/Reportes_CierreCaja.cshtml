
@section CSS{
    <link href="https://kendo.cdn.telerik.com/2020.2.617/styles/kendo.common.min.css" rel="stylesheet" />
    <link href="https://kendo.cdn.telerik.com/2020.2.617/styles/kendo.blueopal.min.css" rel="stylesheet" />
}

@{
    ViewBag.Title = "Reportes Cierre Caja";
}

@{
    if (Convert.ToBoolean(this.Session["Logueado"]) == false)
    {
        Response.Redirect("http://localhost:64943/");
    }
}
<div class="container">
    <div class="jumbotron">
        <h1>@this.ViewBag.Title</h1>
        <p>Reportes de los cierres de caja</p>
    </div>

    <div id="divListaCierreCaja">

    </div>

</div>

<div class="container">
    <h2>Reportes de los Cierres de Caja</h2>
    <div class="form-group">
        <label>Fecha:</label>
        <input type="datetime" name="C_FECHA" id="C_FECHA" />

    </div>

    <input type="button" class="btn btn-success" name="btnConsultar" id="btnConsultar" value="Consultar Fecha" form="frmReportes_CierreCaja" />
</div>

@Html.ValidationSummary(true)
@section Scripts{

    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/kendo.all.min.js"></script>
    <script src="https://kendo.cdn.telerik.com/2020.2.617/js/jszip.min.js"></script>
    <script src="~/Scripts/ScriptGenerico.js"></script>
    <script src="~/Scripts/Reportes/Reporte_CierreCaja.js"></script>
}