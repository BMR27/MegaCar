
@{
    ViewBag.Title = "Lista de los Tipos de vehiculos";
}
<div class="jumbotron">
    <h2>@ViewBag.Title</h2>
</div>
<div class="container">
    <center>
        <form id="frmBuscar" action="TiposVehiculos" method="post">
            <div class="form-group">
                <label>Nombre del tipo</label>
                <input type="text" name="C_NOMBRE_TIPO" id="C_NOMBRE_TIPO" placeholder="Ingrese un nombre" value="" class="form-control" />
            </div>
            <div class="form-group">
                <input type="submit" name="btnBuscar" id="btnBuscar" value="Buscar" class="btn btn-success form-control" />
            </div>
        </form>
    </center>
</div>
<div class="container">
    <table class="table table-responsive">
        <tr>
            <th>Id</th>
            <th>Nombre</th>
            <th>Opciones</th>
        </tr>
        @{
            foreach (ProyectoProgramacion.Modelo.SP_RETORNA_TIPO_VEHICULO_Result item in
                this.ViewBag.ListaTipos)
            {
                <tr>
                    <td>@item.C_ID_TIPO_VEHICULO</td>
                    <td>@item.C_NOMBRE_TIPO</td>
                    <td>
                        <a href="~/TipoVehiculo/ModificarTipoVehiculo?C_ID_TIPO_VEHICULO=@item.C_ID_TIPO_VEHICULO">Modificar</a> <br />
                        <a href="~/TipoVehiculo/EliminarTipoVehiculo?C_ID_TIPO_VEHICULO=@item.C_ID_TIPO_VEHICULO">Eliminar</a>
                    </td>
                </tr>
            }
        }

    </table>
</div>
